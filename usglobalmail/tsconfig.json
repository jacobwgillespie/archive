{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "lib": ["es2016", "dom"],
    "sourceMap": true,
    "declaration": true,
    "rootDir": ".",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "esModuleInterop": true
  }
}
