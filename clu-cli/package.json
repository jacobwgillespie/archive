{
  "name": "clu-cli",
  "version": "0.0.4",
  "description": "Uniformly convert media files",
  "author": "Jacob Gillespie <jacobwgillespie@gmail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/clu-media/clu-cli.git"
  },
  "homepage": "https://github.com/clu-media/clu-cli",
  "bugs": {
    "url": "https://github.com/clu-media/clu-cli/issues"
  },
  "main": "dist/cli.js",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "bin": {
    "clu": "dist/cli.js"
  },
  "scripts": {
    "prepublish": ". ./resources/prepublish.sh",
    "test": "npm run lint",
    "lint": "eslint src",
    "clean": "rimraf dist",
    "build": "npm run clean && babel-node ./node_modules/.bin/webpack --progress --colors --config webpack.config.js",
    "watch": "babel-node ./node_modules/.bin/webpack --progress --colors --watch --config webpack.config.js",
    "start": "node dist/cli.js"
  },
  "dependencies": {
    "babel-polyfill": "^6.7.2",
    "debug": "^2.2.0",
    "es6-error": "^2.1.0",
    "extend": "^3.0.0",
    "faststart": "^1.0.0",
    "fluent-ffmpeg": "fluent-ffmpeg/node-fluent-ffmpeg#master",
    "fs-extra": "^0.26.5",
    "inquirer": "^0.12.0",
    "source-map-support": "^0.4.0",
    "yargs": "^4.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.2",
    "babel-eslint": "^6.0.0-beta.6",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^4.2.1",
    "rimraf": "^2.5.2",
    "webpack": "^1.12.14",
    "webpack-node-externals": "^1.0.0"
  }
}
